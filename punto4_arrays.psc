Algoritmo punto4_arrays
	
	Dimension lista[100]
	definir multip Como Real;
	multip<-0;
	Escribir "Escriba un número entre el 1 hasta 100";
	leer n;
	Para i<-1 Hasta 100 Con Paso 1 Hacer
        Escribir  i;
        Si n MOD 3 = 0 Entonces
            
        FinSi
	FinPara
	escribir "multiplos de 3: ", multip;

FinAlgoritmo
